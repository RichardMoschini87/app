<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RecordsActivity">

    <WebView
        android:id="@+id/web1"
        android:layout_width="484dp"
        android:layout_height="878dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.335" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#CFD6D0"
        tools:context=".MainActivity">

        <TableLayout
            android:id="@+id/table"
            android:layout_width="366dp"
            android:layout_height="479dp"
            android:layout_marginHorizontal="10dp"
            android:layout_marginVertical="30dp"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.262">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="74dp">

                <TextView
                    android:id="@+id/col1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="Workout"
                    android:textColor="#131313"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/col3"
                    android:layout_width="wrap_content"
                    android:layout_height="89dp"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="BestRecord"
                    android:textColor="#131313"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/col2"
                    android:layout_width="158dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="WorstRecord"
                    android:textColor="#131313"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="131dp"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="StrongMan"
                    android:textColor="#151515"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/bestStrongman"
                    android:layout_width="89dp"
                    android:layout_height="112dp"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:textColor="#53CD5C"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/worstStrongman"
                    android:layout_width="125dp"
                    android:layout_height="120dp"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:textColor="#CF2424"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="163dp"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="SixPack"
                    android:textColor="#151515"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/bestSixPack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="TextView"
                    android:textColor="#151515"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />

                <TextView
                    android:id="@+id/worstSixPack"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_marginBottom="1dp"
                    android:fontFamily="cursive"
                    android:gravity="center"
                    android:padding="10sp"
                    android:text="TextView"
                    android:textColor="#151515"
                    android:textSize="18sp"
                    android:textStyle="bold|italic" />
            </TableRow>

        </TableLayout>

        <TextView
            android:id="@+id/training"
            android:layout_width="345dp"
            android:layout_height="80dp"
            android:fontFamily="sans-serif-medium"
            android:gravity="center"
            android:text="RECORDS"
            android:textColor="#010E0C"
            android:textSize="30sp"
            android:textStyle="bold|italic"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.055" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>